# Production Environment Configuration - DEMO VERSION
NODE_ENV=production

# Application URL
NEXT_PUBLIC_BASE_URL=https://ai-customer-intelligence.demo.com

# Supabase Configuration (Demo values - replace with real values)
NEXT_PUBLIC_SUPABASE_URL=https://demo-supabase-url.supabase.co
SUPABASE_SERVICE_ROLE_KEY=demo-service-role-key-replace-with-real-key

# OpenAI API (Demo value - replace with real API key)
OPENAI_API_KEY=demo-openai-api-key-replace-with-real-key

# Slack Integration (Demo values - replace with real tokens)
SLACK_BOT_TOKEN=demo-slack-bot-token-replace-with-real-token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/demo/webhook/url

# Security (Demo values - replace with real secrets)
WEBHOOK_SECRET=demo-webhook-secret-replace-with-real-secret
JWT_SECRET=demo-jwt-secret-replace-with-real-secret

# Monitoring
MONITORING_ENABLED=true
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
REDIS_URL=redis://redis:6379
CACHE_TTL=3600